{
  "name": "Deno",
  "dockerFile": "Dockerfile",
  "settings": {
    // Sets Deno as the default formatter for the project
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  // This will install the vscode-deno extension
  "extensions": [
    "denoland.vscode-deno"
  ],
  "remoteUser": "vscode"
}
